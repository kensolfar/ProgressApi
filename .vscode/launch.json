    "version": "0.2.0",
    "configurations": [

        {
            "name": "Django: Make Migrations",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "makemigrations"
            ],
            "django": true,
            "justMyCode": true,
            "program": "${workspaceFolder}\/manage.py"
        },
        {
            "name": "Django: Migrate",
            "type": "debugpy",
            "request": "launch",
            "args": [
                "migrate"
            ],
            "django": true,
            "justMyCode": true,
            "program": "${workspaceFolder}\/manage.py"
        },
        {
            "name": "Django: Runserver",
            "type": "python",
            "request": "launch",
            "args": [
                "runserver",
                "0.0.0.0:8001"
            ],
            "django": true,
            "justMyCode": true,
            "program": "${workspaceFolder}\/manage.py"
        }
    ]
}